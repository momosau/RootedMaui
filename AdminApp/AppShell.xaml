<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AdminApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AdminApp.Pages"
 Shell.TabBarIsVisible="True"
    Title="AdminApp">
    <TabBar>
    <ShellContent
        Title="Home"
        Icon="home.png"
        ContentTemplate="{DataTemplate local:AdminApproval}"
        Route="AdminApproval" />
    <ShellContent 
        Title="Questions"
        Icon="question.png"
        ContentTemplate="{DataTemplate local:QuestionPage}"
        Route="QuestionPage" />
    </TabBar>
</Shell>
